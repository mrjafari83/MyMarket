<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/png" sizes="16x16" href="/Admin/assets/images/favicon.png">
    <title>پنل ادمین</title>
    <link href="/Admin/dist/css/style.css" rel="stylesheet">
    <link href="/Admin/dist/css/pages/error-pages.css" rel="stylesheet">
</head>

<body class="rtl">
    <section id="wrapper" class="error-page">
        <div class="error-box">
            <div class="error-body center-align">
                <h1>@TempData["ErrorStatusCode"]</h1>
                <h3>@TempData["ErrorMessage"]</h3>
                <a href="/Admin" class="btn btn-round red waves-effect waves-light m-b-40">برگشت به صفحه اصلی</a>
            </div>
        </div>
    </section>
    <!-- ============================================================== -->
    <!-- All Required js -->
    <!-- ============================================================== -->
    <script src="/Admin/assets/libs/jquery/dist/jquery.min.js"></script>
    <script src="/Admin/dist/js/materialize.min.js"></script>
</body>
